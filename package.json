{
  "name": "forms",
  "version": "1.0.0",
  "description": "This is a tiny training application, created to increase my knowledge of React & Redux libs.",
  "main": "./src/index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --open",
    "build": "webpack"
  },
  "author": "Ilya Kuznetsov",
  "license": "ISC",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": "off",
      "strict": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/destructuring-assignment": "off",
      "react/no-did-update-set-state": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-unresolved": [
        2,
        {
          "caseSensitive": false
        }
      ],
      "react/jsx-indent": [
        "error",
        2
      ],
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": false
        }
      ],
      "react/button-has-type": [
        2,
        {
          "button": true,
          "submit": true,
          "reset": true
        }
      ]
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "postcss-modules-values": "^1.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-dev-server": "^3.1.4"
  }
}
